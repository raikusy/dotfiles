Host *
  IgnoreUnknown UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

Host minicabit-bastion
        User rakibul.minicabit
        HostName 52.50.179.210
        IdentityFile ~/.ssh/minicabit

Host minicabit-nat
        User rakibul.minicabit
        HostName 52.208.12.126
        IdentityFile ~/.ssh/minicabit

